{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "es2022",
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./build",
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "lib": [
      "es2022",
      "esnext.asynciterable",
      "dom"
    ],
  },
  "include": [
    "**/*.ts",
    "src/controller/superAdmin/payToVendor"
  ],
  "exclude": [
    "node_modules"
  ]
}